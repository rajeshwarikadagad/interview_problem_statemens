// generate sum of possible sub arrays


function generateSumOfSubArrays(arr) {
  var outPut = [];
  
  for(let i=0; i<arr.length; i++ ) {
    let sarr = [];
    let sum =0;
    for(let j=i; j< arr.length; j++) {
      sarr.push(arr[j]);
      subArr.push([...sarr]);

      sum += arr[j];
      outPut.push(sum); 
  }
  
  } 
  
  return outPut
  }
  
  
  let subArr = []; 
  var arr =[20,-5,-1] ;
  console.log(generateSumOfSubArrays(arr)); 
  console.log(subArr);
