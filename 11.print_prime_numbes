let num= 100;
//if(num == 0 || num ==1) {return console.log('0 ans 1 are not prime numbers')}
for (let i = 2; i <= num; i++) {

  let isPrime = true;
  for (let j = 2; j <= i; j++) {
      if (i%j===0 && j!==i) {
          isPrime = false;
      }
  }
  if (isPrime === true) {  
              console.log(i);
  }
}
