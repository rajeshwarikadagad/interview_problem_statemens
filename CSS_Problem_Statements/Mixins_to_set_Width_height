//
// write a mixin program to set width and height of div

// supports in SCSS

@mixin rect($width, $height) {
  width: $width;
  height: $height;
}

.div1 {
  @include rect(200px,100px);
  background: blue;
  border: 2px solid black;
}


//----------------html--------------------
<div class='div1'></div>
